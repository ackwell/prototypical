
# Direct inheritance of scope
Parent1 = {
	p1 = 'p1';
	Child1 = {
		c1 = p1;
	};
};

out('1:', Parent1().Child1().c1);

# Injected inheritance of scope
Parent2 = {
	p2 = 'p2';
};

Child2 = {
	c2 = p2;
};

Parent2() -> Child2;

out('2:', Child2().c2);

# Chained scope
A = {
	a = 1;
};

B = {
	b = scope.a;
};

out('3:', A().B().b);
